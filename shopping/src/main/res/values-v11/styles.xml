<resources xmlns:android="http://schemas.android.com/apk/res/android">
     
    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <!-- API 11 theme customizations can go here. -->
    </style>
    
    <style name="customButton" parent="@android:style/Widget.Holo.Light.Button.Small">
    </style>
    
    <style name="customListView" parent="@android:style/Widget.Holo.Light.ListView">
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
    </style>

    <style name="spinnerLookingEdit" parent="@android:style/Widget.Holo.Light.Spinner">
        <item name="android:focusable">false</item>
        <item name="android:paddingLeft">@dimen/spinnerLookingEditPadding</item>
        <item name="android:paddingRight">@dimen/spinnerLookingEditPadding</item>
    </style>

</resources>